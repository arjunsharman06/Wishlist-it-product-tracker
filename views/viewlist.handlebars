{{!-- {{products}} --}}
<div class="cutom-row">
<div class='row'>
  <div class="col-md-3" id="left_list-view">
    <a class="title" href="/api/lists/" style="text-decoration: none;">All List </a>
    {{#each lists as |list|}}
   <a class="title" href="/api/lists/{{list.id}}" style="text-decoration: none;">{{list.list_name}} </a>
    {{/each}}
  </div>
  <div class="col-md-9" id="right_list-view">
    <ol class="row" id="parent-product">
      {{#each products as |product|}}
      {{#each product as |product|}}
      
     <div class='col-10 col-md-10 col-lg-4 col m-2'>
  <div
    class='card category-card p-3'
    style='width: 18rem;'
    style='max-height: 400px;'
    class='post'
    data-product-id={{id}}
  >   
    <img
      class='card-img-top'
      src='/images/{{product.image_name}}'
      alt='{{product.product_name}}'
      style=''
    />
    <div class='card-body'>
      <h5 class='card-title'>{{product.product_name}}</h5>
      <p class='card-text' data-toggle="tooltip" data-placement="top" title=`{{product.product_note}}`>{{product.product_note}} </p>
    </div>
    <ol class='list-group list-group-flush'>
      <li class='list-group-item'>Stock: {{product.quantity}}</li>
     
      {{!-- <li class='list-group-item'>Category: {{product.category_name}}</li> --}}
      <li class='list-group-item'>Price: {{product.price}}</li>
    </ol>    
  </div>
</div>
 {{/each}}
      {{/each}}
    </ol>
  </div>
</div>
</div>
